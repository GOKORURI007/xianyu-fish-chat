{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "notification:default",
    "store:default",
    "dialog:default",
    "global-shortcut:default"
  ],
  "remote": {
    "urls": [
      "https://www.goofish.com/*"
    ]
  }
}